{
	"name": "motoko-import-sort",
	"repository": {
		"type": "git",
		"url": "https://github.com/jorgenbuilder/vscode-motoko-import-sort"
	},
	"publisher": "JorgenBuilder",
	"displayName": "motoko-import-sort",
	"description": "Groups Motoko imports into base, 3rd party, project, module and sorts them alphabetically",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:motoko-import-sort.sort"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "motoko-import-sort.sort",
				"title": "Sort Motoko"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
